{
  "name": "@klass/workspace",
  "version": "0.0.0",
  "description": "Class variant utility",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "turbo run clean",
    "build": "set COMMAND=build&&turbo run build",
    "build:unminify": "set UNMINIFY=true&&set COMMAND=build&&turbo run build",
    "test": "set COMMAND=test&&turbo run test",
    "test:watch": "set WATCH=true&&set COMMAND=test&&turbo run test",
    "test:preview": "pnpm --parallel --filter ./packages/** exec vite preview --outDir test-reports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/klass.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/klass/issues"
  },
  "homepage": "https://klass.pages.dev",
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/plugin-transform-react-jsx-development": "^7.22.5",
    "@changesets/cli": "^2.27.1",
    "@preact/preset-vite": "^2.8.1",
    "@solidjs/testing-library": "^0.8.5",
    "@testing-library/preact": "^3.2.3",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.2.0",
    "babel-plugin-transform-hook-names": "^1.0.2",
    "jsdom": "^23.2.0",
    "preact": "^10.19.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "solid-js": "^1.8.10",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vite-plugin-solid": "^2.8.0",
    "vitest": "^1.1.3"
  }
}
