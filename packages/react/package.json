{
  "name": "@klass/react",
  "version": "0.2.2",
  "description": "class variant utility for react",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.m.js",
  "exports": {
    ".": {
      "import": "./dist/index.m.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "microbundle -i src/index.tsx -o dist/index.js -f esm,cjs --target node",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/klass.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/klass/issues"
  },
  "homepage": "https://github.com/flamrdevs/klass#readme",
  "dependencies": {
    "@klass/core": "*",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.9"
  }
}
