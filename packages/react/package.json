{
  "name": "@klass/react",
  "version": "4.0.0-next.26",
  "description": "Class variant utility for React",
  "keywords": [
    "class",
    "classname",
    "variant",
    "variants",
    "utility",
    "responsive",
    "typescript",
    "react",
    "component",
    "styled",
    "polymorphic"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./create": {
      "types": "./dist/create.d.ts",
      "import": "./dist/create.js",
      "require": "./dist/create.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./mono": {
      "types": "./dist/mono/index.d.ts",
      "import": "./dist/mono/index.js",
      "require": "./dist/mono/index.cjs"
    },
    "./mono/create": {
      "types": "./dist/mono/create.d.ts",
      "import": "./dist/mono/create.js",
      "require": "./dist/mono/create.cjs"
    }
  },
  "typesVersions": {
    ">=3.1": {
      "*": [
        "./dist/*.d.ts",
        "./dist/index.d.ts"
      ],
      "mono": [
        "./dist/mono/*.d.ts",
        "./dist/mono/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/klass.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/klass/issues"
  },
  "homepage": "https://klass.pages.dev",
  "peerDependencies": {
    "@klass/core": "4.0.0-next.26",
    "react": "18.2.0 - 19.0.0",
    "react-dom": "18.2.0 - 19.0.0"
  },
  "devDependencies": {
    "@klass/core": "4.0.0-next.26",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
