{
  "name": "@klass/preact",
  "version": "4.0.0-next.14",
  "description": "Class variant utility for Preact",
  "keywords": [
    "class",
    "classname",
    "variant",
    "variants",
    "utility",
    "responsive",
    "typescript",
    "preact",
    "component",
    "styled",
    "polymorphic"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./mono": {
      "import": {
        "types": "./dist/mono.d.ts",
        "default": "./dist/mono.js"
      },
      "require": {
        "types": "./dist/mono.d.ts",
        "default": "./dist/mono.cjs"
      }
    },
    "./setup": {
      "import": {
        "types": "./dist/setup.d.ts",
        "default": "./dist/setup.js"
      },
      "require": {
        "types": "./dist/setup.d.ts",
        "default": "./dist/setup.cjs"
      }
    },
    "./mono.setup": {
      "import": {
        "types": "./dist/mono.setup.d.ts",
        "default": "./dist/mono.setup.js"
      },
      "require": {
        "types": "./dist/mono.setup.d.ts",
        "default": "./dist/mono.setup.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      },
      "require": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.cjs"
      }
    }
  },
  "typesVersions": {
    ">=3.1": {
      "*": [
        "./dist/*.d.ts",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/klass.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/klass/issues"
  },
  "homepage": "https://klass.pages.dev",
  "peerDependencies": {
    "@klass/core": "4.0.0-next.14",
    "preact": "^10.19.0"
  },
  "devDependencies": {
    "@klass/core": "4.0.0-next.14",
    "preact": "^10.19.6"
  }
}
