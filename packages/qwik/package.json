{
  "name": "@klass/qwik",
  "version": "4.0.0-next.3",
  "description": "Class variant utility for Qwik",
  "keywords": [
    "class",
    "classname",
    "variant",
    "variants",
    "utility",
    "responsive",
    "typescript",
    "qwik",
    "component",
    "styled",
    "polymorphic"
  ],
  "type": "module",
  "main": "./dist/index.qwik.cjs",
  "qwik": "./dist/index.qwik.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.qwik.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.qwik.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/klass.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/klass/issues"
  },
  "homepage": "https://klass.pages.dev",
  "peerDependencies": {
    "@builder.io/qwik": "^1.4.0",
    "@klass/core": "4.0.0-next.3"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.4.0",
    "@klass/core": "4.0.0-next.3"
  }
}
