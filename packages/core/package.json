{
  "name": "@klass/core",
  "version": "2.0.3",
  "description": "Class variant utility",
  "keywords": [
    "class",
    "classname",
    "variant",
    "variants",
    "utility",
    "responsive",
    "typescript",
    "modular",
    "framework-agnostic",
    "preact",
    "react",
    "solid"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/klass.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/klass/issues"
  },
  "homepage": "https://klass.pages.dev"
}
