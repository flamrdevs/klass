{
  "name": "@klass/core",
  "version": "4.0.0-next.15",
  "description": "Class variant utility",
  "keywords": [
    "class",
    "classname",
    "variant",
    "variants",
    "utility",
    "responsive",
    "typescript",
    "framework-agnostic",
    "preact",
    "qwik",
    "react",
    "solid",
    "vue"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./setup": {
      "types": "./dist/setup.d.ts",
      "import": "./dist/setup.js",
      "require": "./dist/setup.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./group": {
      "types": "./dist/group/index.d.ts",
      "import": "./dist/group/index.js",
      "require": "./dist/group/index.cjs"
    },
    "./slots": {
      "types": "./dist/slots/index.d.ts",
      "import": "./dist/slots/index.js",
      "require": "./dist/slots/index.cjs"
    }
  },
  "typesVersions": {
    ">=3.1": {
      "*": [
        "./dist/*.d.ts",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/klass.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/klass/issues"
  },
  "homepage": "https://klass.pages.dev",
  "dependencies": {
    "clsx": "^2.0.0"
  }
}
